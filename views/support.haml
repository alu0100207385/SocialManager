%section#support.doublediagonal
  .container
    .section-heading
      %h1 Soporte
      .divider
    .row
      .col-md-12
        .row
          .col-md-8.col-sm-8
            %form{:role => "form"}
              .form-group
                %input.form-control{:placeholder => "Your name", :type => "text"}
              .form-group
                %input.form-control{:placeholder => "Your email", :type => "email"}
              .form-group
                %textarea.form-control{:cols => "30", :placeholder => "Your message", :rows => "10"}
              %button.btn.btn-primary.btn-lg{:type => "submit"} Submit
          .col-md-4.col-sm-4.contact-details.scrollpoint.sp-effect2
            .media
              %a.pull-left{:href => "#"}
                %i.fa.fa-map-marker.fa-2x
              .media-body
                %h4.media-heading , La Laguna, ES
            .media
              %a.pull-left{:href => "#"}
                %i.fa.fa-envelope.fa-2x
              .media-body
                %h4.media-heading
                  %a{:href => "mailto:support@oleose.com"} support@socialmanager.com
            .media
              %a.pull-left{:href => "#"}
                %i.fa.fa-phone.fa-2x
              .media-body
                %h4.media-heading +1 234 567890
